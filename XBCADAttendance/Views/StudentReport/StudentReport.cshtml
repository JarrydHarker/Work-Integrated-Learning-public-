@model StudentReportViewModel

@{
    Layout = "_StudentLayout";
}
       
        <table>
            <thead>
                <tr>
                    <th>Module Code</th>
                    <th>Module Name</th>
                    <th>Lecture Date</th>
                    <th>Attendance</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model.lstLectures)
                { 
                        <tr>
                            <td>@item.ModuleCode</td>
                            <td>Module Name</td>
                            <td>@item.LectureDate</td>                         
                            <td>@Model.GetAttendance(item)</td>
                        </tr>
                    
                }
            </tbody>
        </table>

