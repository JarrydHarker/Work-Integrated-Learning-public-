@await Html.PartialAsync("_StudTopbarPartial")
<div class="custom-container">
    @await Html.PartialAsync("_StudSidebarPartial")
    <div class="custom-content">
        <h1>My Modules</h1>
        <table>
            <thead>
                <tr>Semester </tr>
                <tr>Module Name </tr>
                <tr>Module Code </tr>

            </thead>
            <tbody>
                @if (Model != null)
                {
                    @foreach (var report in Model)
                    {
                        <tr>
                            <td></td>
                            <td></td>
                            <td></td>
                        </tr>
                    }
                }

            </tbody>
        </table>
    </div>
</div>